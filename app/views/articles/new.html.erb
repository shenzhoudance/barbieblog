<h1>Create A  new Articles</h1>
<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %> <br>
    <%= f.text_field :title  %>
  </p>
  <p>
    <%= f.label :text %> <br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<p>
<%= link_to 'back', articles_path %>
<%= link_to 'home', welcome_index_path %>
</p>
